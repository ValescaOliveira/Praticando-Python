#Somatório de todos os números de 1 até 10

soma = 0

for cont in range(1,11):
    soma = soma + cont

print(f"soma: {soma}")


# Somatório de 5 números reais (float) definidos pelo usuário

soma_numeros = 0

for i in range(5):
    num = float(input("digite um número real: "))
    soma_numeros = soma_numeros + num

print(f"soma: {soma_numeros: .2f}")
