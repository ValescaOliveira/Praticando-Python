# Exibir todos os números inteiros entre 1 e 10, inclusive
for cont in range (1,11):
    print(cont)

# Quero exibir todos os números de 0 a 9, inclusive.
for i in range(10):
    print(i)

# Quero exibir todos os núemros de 2 até 20, de 2 em 2 (passo 2)
for cont in range(2,21,2):
   print(cont)


# Exibir somente os número pares de 1 até 20 (passo 1)
for cont in  range(1,21):
   if(cont % 2 == 0):
       print(cont)

# Exibir os número de 10 até 1 (decrescente)
for i in range(10,0,-1):
    print(i)

# Exibir os números de 20 até 2 (decrescente)
for i in range(20,0,-2):
    print(i)
